{
  "name": "hpolthof/laravel-translations-db",
  "description": "A database translations implementation for Laravel 5.",
  "license": "GPL2",
  "keywords": [
    "laravel",
    "translation",
    "localization",
    "database"
  ],
  "authors": [
    {
      "name": "Paul Olthof",
      "email": "hpolthof@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/translation": "7.28.*",
    "illuminate/database": "7.28.*",
    "illuminate/cache": "7.28.*",
    "stichoza/google-translate-php": "4.1.*"
  },
  "autoload": {
    "psr-4": {
      "Hpolthof\\Translation\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hpolthof\\Translation\\ServiceProvider"
      ]
    }
  }
}
